{
  "name": "Bitwarden (Vaultwarden)",
  "version": "dev",
  "slug": "bitwarden",
  "description": "Open source password management solution",
  "url": "https://github.com/hassio-addons/addon-bitwarden",
  "webui": "[PROTO:ssl]://[HOST]:[PORT:7277]",
  "startup": "services",
  "ingress": true,
  "ingress_port": 7277,
  "init": false,
  "arch": ["aarch64", "amd64", "armv7"],
  "ports": {
    "7277/tcp": null
  },
  "ports_description": {
    "7277/tcp": "Vaultwarden Web interface (not required for Ingress)"
  },
  "map": ["ssl"],
  "options": {
    "ssl": true,
    "certfile": "fullchain.pem",
    "keyfile": "privkey.pem"
  },
  "schema": {
    "log_level": "list(trace|debug|info|notice|warning|error|fatal)?",
    "ssl": "bool",
    "certfile": "str",
    "keyfile": "str",
    "request_size_limit": "int?"
  }
}
